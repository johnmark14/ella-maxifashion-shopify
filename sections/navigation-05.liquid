
<style>
.side-bar-menu {
    position: fixed;
    top: 0;
	left: 0;
	width: 100vw;
	height: 100vh;
    /*right: 100%;*/
    z-index: 999;
    overflow-y: scroll;
    -webkit-transition: transform .5s ease;
    -moz-transition: transform .5s ease;
    -ms-transition: transform .5s ease;
    -o-transition: transform .5s ease;
    transition: transform .5s ease;
	overflow: hidden;
	z-index: 1000;
	transform: translateX(-100%);
	opacity: 0;
	transition: opacity .2s linear;
}
.side-bar-menu .close{
	position: absolute;
	width: calc(100% - 370px);
	left: 370px;
	top: 0;
	height: 100%;
	background-color: #32181f6b;
}
.side-bar-menu .jas-mb-style{
	background: #fafbf9;
	padding: theme 25px;
	height: 100vh;
	transform: translateX(-100%);
	transition: transform .2s linear;
	position: relative;
	z-index: 2;
	width: 370px;
	box-shadow: 0px 2px 4px 2px #00000030;
}
.side-bar-menu .dropdown .sub-menu-mobile{
	display: none;
}
.side-bar-menu .main-menu{
	background: #fafbf9;
}
.side-nav-bar .site-nav-side{
	position: relative;
}
.side-nav-bar .site-nav-side li{
	display: block;
	padding: 15px;
	margin-bottom: 10px;
	background: #fff;
}
.side-nav-bar .site-nav-side li span{
	font-weight: 400;
    letter-spacing: .05em;
    font-size: 16px;
    color: #585858;
    letter-spacing: 1.3px;
    text-transform: none;
}
.side-nav-bar .site-nav-side .dropdown .parent-a{
	display: flex;
	justify-content: space-between;
	cursor: pointer;
}
.side-nav-bar .site-nav-side .dropdown .parent-a .icon-dropdown{
	font-size: 24px;
}
.pos-relative-nav-toggle{
	display: block;
	position: relative;
	transform: translateY(6px);
}
.side-bar-menu.active{
	transform: translateX(0);
	opacity: 1;
}
.side-bar-menu.active .jas-mb-style{
	transform: translateX(0);
}
.side-bar-menu .close{
	color: #fff;
}
.side-bar-menu .close svg{
	position: absolute;
	top: 12rem;
	left: 2rem;
	width: 37px;
    height: 37px;
	stroke: #fff;
	color: #fff;
	fill: #fff;
}
.nav-actvive-submenu{
	position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
	background: #fafbf9;
}
.side-actvive-submenu{
	position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
	background: #fafbf9;
	z-index: 10;
	display: block !important;
	padding-top: 15px;
}
.side-actvive-submenu .menu-mb-title{
	padding: 0 0 20px 0;
	cursor: pointer;
}
.nav-bar.side-nav-bar{
	padding: 25px;
}
@media screen and (max-width:640px){
	.nav-bar.side-nav-bar{
		padding: 0;
	}
}
.item.img{
	border-bottom: 1px solid #dfdfdf;
}
.item.img .img-h img{
	max-width: 100%;
}
.item.img .img-h{
	height: 40px;
	width: 40px;
	border-radius: 50%;
	overflow: hidden;
	display: inline-block;
	margin-right: 10px;
}
.item.img a{
	display: flex;
	align-items: center;
}
.nav-bar.side-nav-bar.actvive-submenu .site-nav-side .item:not(.nav-actvive-submenu){
	display: none;
}
.nav-bar.side-nav-bar.actvive-submenu .site-nav-side .nav-actvive-submenu{
	position: relative;
	height: auto;
	background: transparent;
	padding: 0;
}
.nav-bar.side-nav-bar.actvive-submenu .site-nav-side .nav-actvive-submenu .parent-a{
	display: none;
}
.nav-bar.side-nav-bar.actvive-submenu .site-nav-side .nav-actvive-submenu .side-actvive-submenu{
	position: relative;
	height: auto;
}
.side-nav-bar .site-nav-side .addi-heading-outher{
	background: no-repeat;
    border: none;
    padding: 0;
	padding-top: 10px;
}
.side-nav-bar .site-nav-side .addi-heading-outher .addi-heading{
	color: #222;
}
