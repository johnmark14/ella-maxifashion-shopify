<script>
    window.fixtop_menu = {{ section.settings.display_fixtop_menu }};
	const toggler = document.querySelectorAll(".pos-relative-nav-toggle");
	const _body = document.querySelector("body");
	
	const sideNav = document.querySelector(".side-bar-menu");
	const closeBtn = document.querySelector(".side-bar-menu .close");

	const parentDropDownList = document.querySelectorAll(".site-nav-side .dropdown");

	const dropDownCloseBtnList = document.querySelector(".sub-menu-mobile .menu-mb-title");

	for (let i = 0; i < dropDownCloseBtnList.length; i++) {
		dropDownCloseBtnList[i].addEventListener('click', function(event) {
			let element = event.target;
			const parent = element.closest(".dropdown");
			const parentId = parent.id;
			let subMenu = document.querySelector("#"+ parentId +" .sub-menu-mobile");
			let holder = document.querySelector(".nav-bar.side-nav-bar");


			holder.classList.toggle("actvive-submenu");
			parent.classList.toggle("nav-actvive-submenu");
			subMenu.classList.toggle("side-actvive-submenu");
		});
	}

	for (let i = 0; i < parentDropDownList.length; i++) {
		parentDropDownList[i].addEventListener('click', function(event) {

			let element = event.target;
			const parent = element.closest(".dropdown");
			const parentId = parent.id;
			let subMenu = document.querySelector("#"+ parentId +" .sub-menu-mobile");
			let holder = document.querySelector(".nav-bar.side-nav-bar");
			holder.classList.toggle("actvive-submenu");
			parent.classList.toggle("nav-actvive-submenu");
			subMenu.classList.toggle("side-actvive-submenu");
		});
	}
	for (let i = 0; i < toggler.length; i++) {
		toggler[i].addEventListener('click', function(event) {
			event.preventDefault;
			toggler[i].classList.toggle("toggled");
			sideNav.classList.toggle("active");
			_body.classList.toggle("ov-hidden");
		});
	}


	closeBtn.addEventListener('click', function(event) {
		sideNav.classList.toggle("active");
		_body.classList.toggle("ov-hidden");
		for (let i = 0; i < toggler.length; i++) {
			toggler[i].classList.toggle("toggled");
		}
	});

</script>

